   "source": [
    "# Realizar iteraciones en llaves, valores y objetos de un diccionario\n",
    "d = {'k1':1,'k2':2}\n",
    "\n",
    "for llaves in d.keys():\n",
    "    print(llaves)\n",
    "    \n",
    "for valores in d.values():\n",
    "    print(valores)\n",
    "    \n",
    "for objeto in d.items():\n",
    "    print(objeto)"
   ]
